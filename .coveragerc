[run]
source = src/linkedin_lead_scoring
omit =
    */tests/*
    */conftest.py
    */__pycache__/*
    */site-packages/*

[report]
precision = 2
skip_empty = True
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
